{
  "name": "qrlinkki.web",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "start:clear": "expo start --clear",
    "generate:icons": "node ./scripts/generate-icons.js",
    "build:android:release": "powershell -ExecutionPolicy Bypass -File ./scripts/build-android-release.ps1",
    "build:android:dev": "powershell -ExecutionPolicy Bypass -File ./scripts/build-android-dev.ps1",
    "build:android:staging": "powershell -ExecutionPolicy Bypass -File ./scripts/build-android-staging.ps1",
    "android": "expo run:android",
    "android:dev": "expo run:android --device",
    "android:emulator": "expo run:android",
    "android:logs": "adb logcat -s ReactNativeJS:V ReactNative:V *:E",
    "dev:android": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-android.ps1",
    "dev:android:clear": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-android.ps1 -Clear",
    "dev:android:logs": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-android.ps1 -Logs",
    "dev:android:full": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-android.ps1 -Clear -Logs",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios --device",
    "ios:simulator": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "type-check": "tsc --noEmit",
    "postinstall": "node ./scripts/fix-expo-secure-store.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "dotenv": "^16.3.1",
    "expo": "~54.0.23",
    "expo-camera": "~17.0.9",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2",
    "expo-module-scripts": "^5.0.0",
    "expo-dev-client": "~6.0.17"
  },
  "private": true
}
